<?xml version="1.0" ?>
<freevo>

   <skin include="800x600">

    <!-- Settings for a normal menu -->
    <menu type="all">

      <background image="../images/aubin_bg1.png"/>

      <title visible="yes" x="384">
	<font size="34"/>
      </title>

      <items y="105" height="370" width="620"/>

      <covers>
	<cover type="all" x="538"/>
      </covers>

      <submenu y="500"/>

    </menu>

    <!-- Override some settings for the main menu -->
    <menu type="main">
      <items x="230" width="320"/>
    </menu>


    <!-- Settings for the mp3 player -->
    <mp3>
      <title x="384"/>
      <background image="../images/aubin_bg1.png"/>
      <cover visible="yes" x="466" y="191" width="289"
	height="289" border_color="0x000000" border_size="1"/> 
      <progressbar y="490" width="768"/>
    </mp3>


    <!-- Settings for a popup box -->
    <popup bgcolor="0x33ffffff" border_size="2" border_color="0x000000">
      <message width="586" height="376"/>
    </popup>

  </skin>
  
</freevo>
