#! /sbin/runscript
# Boot script for freevoweb on gentoo
# by den_RDC


depend() {
        need localmount net
}

start() {
        ebegin "Starting freevo record_server"
                /usr/bin/freevo execute record_server start \
		    &> /dev/null &
        eend
}

stop() {
        ebegin "Stopping freevo record_server"
                /usr/bin/freevo execute record_server stop \
		    &> /dev/null
        eend
}

