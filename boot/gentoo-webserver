#! /sbin/runscript
# Boot script for freevoweb on gentoo
# by den_RDC


depend() {
        need localmount net
}

start() {
        ebegin "Starting freevo webserver"
                /usr/bin/freevo execute webserver start \
		    &> /dev/null &
        eend
}

stop() {
        ebegin "Stopping freevo webserver"
                /usr/bin/freevo execute webserver stop \
		    &> /dev/null
        eend
}

