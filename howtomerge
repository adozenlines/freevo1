#!/bin/bash
last=9238

LAST=${1:-${last}}
cd /sources/svn/freevo-1.x/freevo
echo "last change is: ${LAST}"
svn merge -r${LAST}:HEAD /sources/svn/freevo-1.6/ . --dry-run
echo "press enter to do it"
read ENTER
svn merge -r${LAST}:HEAD /sources/svn/freevo-1.6/ .

