# ======================================================================
# Freevo weather plugin:
# ======================================================================

plugin.activate('weather', level=45)
PLUGIN_WEATHER_LOCATIONS = [ ("@PLUGIN_WEATHER_LOCATIONS.text", 1, "Weather-Code") ]

EOFWETTERCFG
@execEnd
@endif
######################################################################################################
@if(@Plugin_news.checked)
