#################################################################
##
## Emuexec-Joystick-Wrapper
##

import joystick
plugin.activate('joy')
JOY_DEV = joystick.JOY_DEV
JOY_CMDS = joystick.JOY_CMDS


EOFJOYCFG
@execEnd
@endif


@if(@Plugin_www.checked)
@execBegin
cat &lt;&lt;EOFWWWCFG&gt;&gt;@env(HOME)/.freevo/local_conf.py
# ======================================================================
# Freevo builtin WWW server settings:
# ======================================================================

plugin.activate('www')

WWW_PORT = @WWW_PORT.checked
WWW_USERS = { "@WWW_USER.text" : "@WWW_PASS.text" }
WWW_GUIDE_INTERVAL = 30
WWW_GUIDE_PRECISION = 5
WWW_GUIDE_COLS = 6
WWW_STYLESHEET = 'styles/main.css'
WWW_JAVASCRIPT = 'scripts/display_prog-head.js'

EOFWWWCFG
@execEnd
@endif
######################################################################################################
@if(@Plugin_wetter.checked)
@execBegin
