cat &lt;&lt;EOFCFG&gt;@env(HOME)/.freevo/local_conf.py
#if 0
# -----------------------------------------------------------------------
# freevo_config.py - System configuration generated by qserver-mod
# -----------------------------------------------------------------------
#endif

CONFIG_VERSION = @CONFIG_VERSION.text

# ======================================================================
# General freevo settings:
# ======================================================================
### Audio
AUDIO_DEVICE           = '@AUDIO_DEVICE.text'
AUDIO_INPUT_DEVICE     = '@AUDIO_INPUT_DEVICE.text'
DEV_MIXER              = '@DEV_MIXER.text'
MAJOR_AUDIO_CTRL       = 'VOL'             # Freevo takes control over one audio ctrl
CONTROL_ALL_AUDIO      = 1                 # Should Freevo take complete control of audio
MAX_VOLUME             = @MAX_VOLUME.text                # Set what you want maximum volume level to be.
DEFAULT_VOLUME         = @DEFAULT_VOLUME.text                # Set default volume level.
TV_IN_VOLUME           = @TV_IN_VOLUME.text                # Set this to your preferred level 0-100.
VCR_IN_VOLUME          = @VCR_IN_VOLUME.text                # If you use different input from TV
RADIO_IN_VOLUME        = @RADIO_IN_VOLUME.text                # If you use different input from Radio

### Main
SKIN_XML_FILE          = '@SKIN_XML_FILE.text'
START_FULLSCREEN_X     = @START_FULLSCREEN_X.checked
HIDE_UNUSABLE_DISCS    = @HIDE_UNUSABLE_DISCS.checked
MENU_ARROW_NAVIGATION  = @MENU_ARROW_NAVIGATION.checked
SYS_USE_NETWORK            = @SYS_USE_NETWORK.checked
ROM_SPEED              = @ROM_SPEED.text

### ShutDown and Reboot
CONFIRM_SHUTDOWN       = @CONFIRM_SHUTDOWN.checked
SYS_SHUTDOWN_ENABLE    = @SYS_SHUTDOWN_ENABLE.checked
SYS_SHUTDOWN_CMD = '@SYS_SHUTDOWN_CMD.text'
SYS_RESTART_CMD  = '@SYS_RESTART_CMD.text'

### Network-Remote
REMOTE_CONTROL_HOST    = '@REMOTE_CONTROL_HOST.text'
REMOTE_CONTROL_PORT    = @REMOTE_CONTROL_PORT.text
ENABLE_NETWORK_REMOTE  = @ENABLE_NETWORK_REMOTE.checked

### Cache
CACHE_IMAGES           = @CACHE_IMAGES.checked
MEDIAINFO_USE_MEMORY   = @MEDIAINFO_USE_MEMORY.checked
MMPYTHON_CREATE_MD5_ID = @MMPYTHON_CREATE_MD5_ID.checked

### Overlay
OVERLAY_DIR            = os.path.join(FREEVO_CACHEDIR, '@env(HOME)/.freevo/cache')
UMASK                  = 022

### Sorting
DIRECTORY_SORT_BY_DATE = @DIRECTORY_SORT_BY_DATE.currentItem
DIRECTORY_REVERSE_SORT = @DIRECTORY_REVERSE_SORT.checked
DIRECTORY_SMART_SORT   = @DIRECTORY_SMART_SORT.checked

### Tag/Autoplay
DIRECTORY_AUTOPLAY_SINGLE_ITEM = @DIRECTORY_AUTOPLAY_SINGLE_ITEM.checked
DIRECTORY_USE_MEDIAID_TAG_NAMES = @DIRECTORY_USE_MEDIAID_TAG_NAMES.checked

### Ripping
plugin.activate('audio.cdbackup')
AUDIO_BACKUP_DIR = '@AUDIO_BACKUP_DIR.text'
CDPAR_CMD = 'cdparanoia'
LAME_CMD = 'lame'
OGGENC_CMD ='oggenc'

### Image-Viewer Options
IMAGEVIEWER_BLEND_MODE = @expr(@IMAGEVIEWER_BLEND_MODE.currentItem - 1)
IMAGEVIEWER_DURATION = @IMAGEVIEWER_DURATION.text
IMAGE_USE_EXIF_THUMBNAIL = @IMAGE_USE_EXIF_THUMBNAIL.checked

# ======================================================================
# Freevo Media Items:
# ======================================================================

@exec(sh "$HOME/.freevo/generator/generic_array" VIDEO_ITEMS video_items)

@exec(sh "$HOME/.freevo/generator/generic_array" AUDIO_ITEMS audio_items)

@exec(sh "$HOME/.freevo/generator/generic_array" IMAGE_ITEMS image_items)

# ======================================================================
# Freevo Suffix Settings:
# ======================================================================

@exec(sh "$HOME/.freevo/generator/generic" VIDEO_MPLAYER_SUFFIX mplayer_suffixes)
@exec(sh "$HOME/.freevo/generator/generic" VIDEO_XINE_SUFFIX xine_suffixes)

@exec(sh "$HOME/.freevo/generator/generic" AUDIO_SUFFIX audio_suffixes)
@exec(sh "$HOME/.freevo/generator/generic" IMAGE_SUFFIX image_suffixes)
@exec(sh "$HOME/.freevo/generator/generic" PLAYLIST_SUFFIX playlist_suffixes)

TV_RECORD_DIR = [ '@TV_RECORD_DIR.text' ]
VIDEO_SHOW_DATA_DIR = 0

# ======================================================================
# Freevo Player settings:
# ======================================================================
### Generic Options
VIDEO_PREFERED_PLAYER  = '@VIDEO_PREFERED_PLAYER.text'
AUDIO_PREFERED_PLAYER  = '@AUDIO_PREFERED_PLAYER.text'
AUDIO_SHOW_VIDEOFILES  = @AUDIO_SHOW_VIDEOFILES.checked

### Generic Video Options
VIDEO_AUTOJOIN = @VIDEO_AUTOJOIN.checked
VIDEO_INTERLACING = @VIDEO_INTERLACING.checked

### MPlayer Options
MPLAYER_AO_DEV       = '@AUDIO_DRIVER.text'
MPLAYER_VO_DEV       = '@VIDEO_DRIVER.text'
MPLAYER_AUTOCROP     = @MPLAYER_AUTOCROP.checked

DVD_LANG_PREF        = '@DVD_LANG_1.text,@DVD_LANG_2.text,@DVD_LANG_3.text'
DVD_SUBTITLE_PREF    = '@DVD_SUB_1.text,@DVD_SUB_2.text,@DVD_SUB_3.text'
MPLAYER_ARGS = { 'dvd'    : '-cache 8192',
                  'vcd'    : '-cache 4096',
                  'cd'     : '-cache @CDDA_ROM_CACHE.text -cdda speed=@CDDA_ROM_SPEED.text',
                  'tv'     : '-nocache',
                  'ivtv'   : '-cache 8192',
                  'avi'    : '-cache 5000 -idx',
                  'rm'     : '-cache 5000 -forceidx',
                  'rmvb'   : '-cache 5000 -forceidx',
                  'webcam' : 'tv:// -tv driver=v4l:width=352:height=288:outfmt=yuy2:device=@VIDEO_WEBCAM_DEVICE.text',
                  'default': '-cache 5000'
                  }

### Xine Options
XINE_AO_DEV = '@AUDIO_DRIVER.text'
XINE_VO_DEV  = '@VIDEO_DRIVER.text'

XINE_COMMAND = '%s --hide-gui -pq -g -B --geometry @VIDEO_RESOLUTION.text+0+0 --no-splash' % \
                  (CONF.xine)

XINE_ARGS_DEF = '--no-lirc --post=pp:quality=10,expand'


# ======================================================================
# Freevo Event settings:
# ======================================================================

EVENTS['menu']['2'] = Event(MENU_UP, arg='')
EVENTS['menu']['4'] = Event(MENU_LEFT, arg='')
EVENTS['menu']['6'] = Event(MENU_RIGHT, arg='')
EVENTS['menu']['8'] = Event(MENU_DOWN, arg='')
EVENTS['menu']['0'] = Event(MENU_CHANGE_STYLE, arg='')
EVENTS['menu']['5'] = Event(MENU_PLAY_ITEM, arg='')
EVENTS['menu']['7'] = Event(MENU_SELECT, arg='')
EVENTS['menu']['9'] = Event(MENU_BACK_ONE_MENU, arg='')
EVENTS['image']['0'] = Event(STOP, arg='')
EVENTS['image']['1'] = Event(PLAYLIST_PREV, arg='')
EVENTS['image']['2'] = Event(STOP, arg='')
EVENTS['image']['3'] = Event(PLAYLIST_NEXT, arg='')
EVENTS['audio']['0'] = Event(STOP, arg='')
EVENTS['audio']['1'] = Event(PLAYLIST_PREV, arg='')
EVENTS['audio']['2'] = Event(STOP, arg='')
EVENTS['audio']['3'] = Event(PLAYLIST_NEXT, arg='')
EVENTS['video']['0'] = Event(STOP, arg='')
EVENTS['video']['1'] = Event(PLAYLIST_PREV, arg='')
EVENTS['video']['2'] = Event(STOP, arg='')
EVENTS['video']['3'] = Event(PLAYLIST_NEXT, arg='')
EVENTS['tvmenu']['0'] = Event(STOP, arg='')
EVENTS['tvmenu']['1'] = Event(PLAYLIST_PREV, arg='')
EVENTS['tvmenu']['2'] = Event(STOP, arg='')
EVENTS['tvmenu']['3'] = Event(PLAYLIST_NEXT, arg='')


# ======================================================================
# Freevo Plugins:
# ======================================================================

@exec(sh "$HOME/.freevo/generator/plugin" @Plugin_audio_playlist.checked audio playlist)
@exec(sh "$HOME/.freevo/generator/plugin" @Plugin_audio_radio.checked audio radio )
@exec(sh "$HOME/.freevo/generator/plugin" @Plugin_audio_mplayervis.checked audio mplayervis )
@exec(sh "$HOME/.freevo/generator/plugin" @Plugin_video_dvdimage.checked video dvdimage )
@exec(sh "$HOME/.freevo/generator/plugin" @Plugin_ossmixer.checked ossmixer )
plugin.activate('df')
plugin.activate('idlebar.logo')
plugin.activate('idlebar.volume')
plugin.remove('tv')


# ======================================================================
# External wrapper functions:
# ======================================================================

import sys
sys.path.insert (0 ,'@env(HOME)/.freevo')

EOFCFG
@execEnd

@if(@Wrap_Gameconf.checked)
@execBegin
test -e "@env(HOME)/.freevo/emuexec.py" &amp;&amp; cat &lt;&lt;EOFEECFG&gt;&gt;@env(HOME)/.freevo/local_conf.py
#################################################################
##
## EmuExec-Wrapper
##

import emuexec
GAMES_NICE = emuexec.GAMES_NICE
GAMES_ITEMS = emuexec.GAMES_ITEMS


EOFEECFG
@execEnd
@endif

@if(@Wrap_Joyconf.checked)
@execBegin
